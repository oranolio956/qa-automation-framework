      "arguments": {
        "x-message-ttl": 600000,
        "x-max-priority": 10
      }
    },
    {
      "name": "sms_queue_p3",
      "vhost": "antibot_vhost",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 900000,
