{
  "rabbit_version": "3.12.0",
  "rabbitmq_version": "3.12.0",
  "product_name": "RabbitMQ",
  "product_version": "3.12.0",
  "users": [
    {
      "name": "antibot",
      "password_hash": "gS4mdKxSfUHwHwX8nFdOmO2A6qjCOV3VXOgOTJKmOGPUEh1V",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": "administrator"
    }
  ],
  "vhosts": [
    {
      "name": "antibot_vhost"
    }
  ],
  "permissions": [
    {
      "user": "antibot",
      "vhost": "antibot_vhost",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "topic_permissions": [],
  "parameters": [],
  "global_parameters": [
    {
      "name": "cluster_name",
      "value": "antibot-cluster"
    }
  ],
  "policies": [
    {
      "vhost": "antibot_vhost",
      "name": "ha-policy",
      "pattern": "^(sms|risk|data|audit).*",
      "apply-to": "queues",
      "definition": {
        "ha-mode": "exactly",
        "ha-params": 2,
        "ha-sync-mode": "automatic",
        "ha-sync-batch-size": 1000
      },
      "priority": 100
    },
    {
      "vhost": "antibot_vhost", 
      "name": "dlx-policy",
      "pattern": "^(sms|risk|data)_queue$",
      "apply-to": "queues",
      "definition": {
        "dead-letter-exchange": "dlx",
        "dead-letter-routing-key": "failed",
        "message-ttl": 3600000
      },
      "priority": 50
    },
    {
      "vhost": "antibot_vhost",
      "name": "priority-queue-policy", 
      "pattern": "^priority_.*",
      "apply-to": "queues",
      "definition": {
        "max-priority": 10,
        "ha-mode": "exactly",
        "ha-params": 2
      },
      "priority": 75
    }
  ],
  "queues": [
    {
      "name": "sms_queue",
      "vhost": "antibot_vhost",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 3600000,
        "x-max-priority": 10,
        "x-dead-letter-exchange": "dlx",
        "x-dead-letter-routing-key": "sms.failed"
      }
    },
    {
      "name": "sms_queue_p1",
      "vhost": "antibot_vhost", 
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 300000,
        "x-max-priority": 10
