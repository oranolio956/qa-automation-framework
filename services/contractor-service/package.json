{
  "name": "contractor-service",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "migrate": "ts-node-dev --transpile-only src/migrate.ts",
    "seed": "ts-node-dev --transpile-only src/seed.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "test": "jest --passWithNoTests",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "express": "4.19.2",
    "pg": "8.12.0",
    "pg-pool": "3.6.2",
    "zod": "3.23.8",
    "node-fetch": "3.3.2"
  },
  "devDependencies": {
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.12",
    "@types/node": "22.5.3",
    "@types/supertest": "2.0.16",
    "jest": "29.7.0",
    "supertest": "7.0.0",
    "ts-node-dev": "2.0.0",
    "typescript": "5.5.4"
  }
}