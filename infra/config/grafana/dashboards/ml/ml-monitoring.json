{
  "dashboard": {
    "id": null,
    "title": "ML Model Performance Monitoring",
    "tags": ["ml", "models", "performance", "antibot"],
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 1,
    "time": {
      "from": "now-3h",
      "to": "now"
    },
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Model Accuracy",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "ml_model_accuracy",
            "legendFormat": "{{model_name}} v{{model_version}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "decimals": 3,
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 0.85},
                {"color": "green", "value": 0.90}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Prediction Latency",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(ml_model_latency_seconds_bucket[5m])) * 1000",
            "legendFormat": "95th Percentile",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "decimals": 1,
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 50},
                {"color": "red", "value": 100}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Model Throughput",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "ml_model_throughput_rps",
            "legendFormat": "{{model_name}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "decimals": 1,
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 4,
        "title": "Data Quality Score",
        "type": "gauge",
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "ml_data_quality_score",
            "legendFormat": "{{model_name}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 0.8},
                {"color": "green", "value": 0.95}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Model Performance Over Time",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "ml_model_accuracy",
            "legendFormat": "Accuracy - {{model_name}}",
            "refId": "A"
          },
          {
            "expr": "ml_false_positive_rate",
            "legendFormat": "FPR - {{model_name}}",
            "refId": "B"
          },
          {
            "expr": "ml_false_negative_rate",
            "legendFormat": "FNR - {{model_name}}",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "fillOpacity": 10
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byRegexp", "options": "Accuracy.*"},
              "properties": [
                {"id": "color", "value": {"mode": "fixed", "fixedColor": "green"}}
              ]
            },
            {
              "matcher": {"id": "byRegexp", "options": "FPR.*"},
              "properties": [
                {"id": "color", "value": {"mode": "fixed", "fixedColor": "red"}}
              ]
            },
            {
              "matcher": {"id": "byRegexp", "options": "FNR.*"},
              "properties": [
                {"id": "color", "value": {"mode": "fixed", "fixedColor": "orange"}}
              ]
            }
          ]
        }
      },
      {
        "id": 6,
        "title": "Feature Drift Scores",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "ml_feature_drift_score",
            "legendFormat": "{{feature_name}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "fillOpacity": 5
            },
            "thresholds": {
              "steps": [
                {"color": "transparent", "value": null},
                {"color": "red", "value": 0.3}
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "title": "Prediction Distribution",
        "type": "histogram",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "rate(ml_risk_score_distribution_bucket[5m])",
            "legendFormat": "Risk Score {{le}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "custom": {
              "drawStyle": "bars",
              "barAlignment": 0,
              "fillOpacity": 80
            }
          }
        }
      },
      {
        "id": 8,
        "title": "Model Comparison",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "ml_model_accuracy",
            "legendFormat": "",
            "refId": "A",
            "format": "table"
          },
          {
            "expr": "ml_false_positive_rate",
            "legendFormat": "",
            "refId": "B",
            "format": "table"
          },
          {
            "expr": "ml_false_negative_rate",
            "legendFormat": "",
            "refId": "C",
            "format": "table"
          },
          {
            "expr": "histogram_quantile(0.95, rate(ml_model_latency_seconds_bucket[5m]))",
            "legendFormat": "",
            "refId": "D",
            "format": "table"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "auto",
              "filterable": true
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "model_name"},
              "properties": [
                {"id": "displayName", "value": "Model Name"},
                {"id": "custom.width", "value": 150}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "model_version"},
              "properties": [
                {"id": "displayName", "value": "Version"},
                {"id": "custom.width", "value": 100}
              ]
            },
            {
              "matcher": {"id": "byRegexp", "options": "Value.*A"},
              "properties": [
                {"id": "displayName", "value": "Accuracy"},
                {"id": "unit", "value": "percentunit"},
                {"id": "decimals", "value": 3}
              ]
            },
            {
              "matcher": {"id": "byRegexp", "options": "Value.*B"},
              "properties": [
                {"id": "displayName", "value": "FPR"},
                {"id": "unit", "value": "percentunit"},
                {"id": "decimals", "value": 4}
              ]
            },
            {
              "matcher": {"id": "byRegexp", "options": "Value.*C"},
              "properties": [
                {"id": "displayName", "value": "FNR"},
                {"id": "unit", "value": "percentunit"},
                {"id": "decimals", "value": 4}
              ]
            },
            {
              "matcher": {"id": "byRegexp", "options": "Value.*D"},
              "properties": [
                {"id": "displayName", "value": "P95 Latency"},
                {"id": "unit", "value": "s"},
                {"id": "decimals", "value": 3}
              ]
            }
          ]
        }
      },
      {
        "id": 9,
        "title": "A/B Testing Results",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "rate(ml_predictions_processed_total[5m])",
            "legendFormat": "{{model_name}} v{{model_version}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "fillOpacity": 10
            }
          }
        }
      },
      {
        "id": 10,
        "title": "Model Drift Alerts",
        "type": "logs",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
        "targets": [
          {
            "expr": "{job=\"ml-monitor\"} |= \"drift\"",
            "refId": "A"
          }
        ],
        "options": {
          "showTime": true,
          "showLabels": false,
          "showCommonLabels": false,
          "wrapLogMessage": false,
          "prettifyLogMessage": false,
          "enableLogDetails": true,
          "dedupStrategy": "none",
          "sortOrder": "Descending"
        }
      }
    ],
    "templating": {
      "list": [
        {
          "name": "model_name",
          "type": "query",
          "datasource": "Prometheus",
          "refresh": 1,
          "regex": "",
          "query": "label_values(ml_model_accuracy, model_name)",
          "multi": true,
          "includeAll": true
        },
        {
          "name": "model_version",
          "type": "query",
          "datasource": "Prometheus",
          "refresh": 1,
          "regex": "",
          "query": "label_values(ml_model_accuracy{model_name=\"$model_name\"}, model_version)",
          "multi": true,
          "includeAll": true
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Model Deployments",
          "datasource": "Prometheus",
          "enable": true,
          "expr": "changes(ml_model_accuracy[1m]) != 0",
          "iconColor": "blue",
          "titleFormat": "Model Update",
          "textFormat": "{{model_name}} v{{model_version}} deployed"
        },
        {
          "name": "Drift Alerts",
          "datasource": "Prometheus",
          "enable": true,
          "expr": "ml_feature_drift_score > 0.3",
          "iconColor": "orange",
          "titleFormat": "Feature Drift Alert",
          "textFormat": "{{feature_name}} drift score: {{value}}"
        }
      ]
    }
  }
}